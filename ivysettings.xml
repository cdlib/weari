<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>  
  <settings defaultResolver="chained"/>
  <resolvers>
    <chain name="chained" returnFirst="true">
      <url name="java.net" m2compatible="true"
           checksums=""> <!-- ridiculous, broken checksums -->
        <ivy pattern="http://download.java.net/maven/2/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).pom"/>
        <artifact pattern="http://download.java.net/maven/2/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
      </url>
      <ibiblio name="ibiblio" m2compatible="true"
           checksums="" /> <!-- ridiculous, broken checksums -->
      <ibiblio name="shorrockin.com" m2compatible="true"
               root="http://maven.shorrockin.com/"/>
    </chain>
  </resolvers>
</ivysettings>